<uwsgi>
	<socket>:8000</socket>
	<master>true</master>
	<chdir>./</chdir>
	<env>DJANGO_SETTINGS_MODULE=Only.settings</env>
	<module>django.core.wsgi:get_wsgi_application()</module>
	<processes>2</processes>
	<daemonize>uwsgi.log</daemonize>
    <pidfile>./uwsgi.pid</pidfile>
	<plugins>python</plugins>
</uwsgi>
